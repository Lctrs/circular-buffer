{
  "name": "lctrs/circular-buffer",
  "description": "Provides a circular buffer implementation.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "circular",
    "buffer",
    "queue",
    "ring",
    "cyclic"
  ],
  "authors": [
    {
      "name": "Jérôme Parmentier",
      "email": "jerome@prmntr.me"
    }
  ],
  "homepage": "https://github.com/Lctrs/circular-buffer",
  "support": {
    "issues": "https://github.com/Lctrs/circular-buffer/issues",
    "source": "https://github.com/Lctrs/circular-buffer"
  },
  "require": {
    "php": "~8.0.2 || ~8.1.0 || ~8.2.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "12.0.0",
    "ergebnis/composer-normalize": "2.31.0",
    "ergebnis/license": "2.4.0",
    "infection/infection": "0.26.19",
    "maglnet/composer-require-checker": "4.4.0",
    "phpstan/extension-installer": "1.3.1",
    "phpstan/phpstan": "1.10.57",
    "phpstan/phpstan-deprecation-rules": "1.1.4",
    "phpstan/phpstan-phpunit": "2.0.6",
    "phpstan/phpstan-strict-rules": "1.5.2",
    "phpunit/phpunit": "9.6.16",
    "psalm/plugin-phpunit": "0.18.4",
    "vimeo/psalm": "5.20.0"
  },
  "autoload": {
    "psr-4": {
      "Lctrs\\CircularBuffer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lctrs\\CircularBuffer\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.0.2"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
